{
  "website":"https://tasklist-9876.herokuapp.com/",
  "repository":"https://github.com/handaman-job/kadai-tasklist",
  "logo":"https://kids.techacademy.jp/assets/common/logo-kids-white-abb5e84177ce74fd7ccc32f9e5391573878e8536eca4742f79d65575655a666e.png",
  "addons": [
    "heroku-postgresql"
  ],
  "buildpacks": [
    {
      "url": "heroku/ruby"
    }
  ],
  "env": {
    "SECRET_TOKEN":{
      "description":"A secret key for verifying the integrity of signed cookies.",
      "generator":"secret"
    }
  },
  "formation": {
    "web": {
      "quantity": 1
    }
  },
  "name": "kadai-tasklist",
  "scripts": {
    "postdeploy": "bundle exec rails db:migrate"
  },
  "stack": "heroku-18",
  
  "environments": {
    "test": {
      "addons": [
        "heroku-postgresql"
      ],
      "scripts": {
        "test": ""
      }
    }
  }
}

